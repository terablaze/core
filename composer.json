{
  "name": "terablaze/core",
  "description": "TeraBlaze",
  "keywords": ["php", "mvc", "framework", "terablaze"],
  "homepage": "https://github.com/terablaze/terablaze",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomiwa Ibiwoye",
      "email": "tomiwa@teraboxx.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "psr/container": "^1.0@dev",
    "terablaze/psr7": "dev-master",
    "terablaze/psr7-server": "dev-master",
    "laminas/laminas-httphandlerrunner": "^1.1@dev",
    "relay/relay": "2.x-dev"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "TeraBlaze\\": "src/"
    },
    "files": [
      "src/TeraBlaze/Functions.php",
      "src/TeraBlaze/Router/Generator/path.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\TeraBlaze\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^5.1@dev",
    "phpunit/phpunit": "^9"
  }
}
